    "query": {
        "from": {
            "table": "Games\\Categories\\Code\\Tables\\Categories",
            "alias": "gc",
            "name": "#__games_categories"
        },
        "select": [
            "gc.id",
            "gc.title",
            "gc.icon",
            "mm.title AS icon_title",
            "mm.file AS icon_file",
            "gc.description",
            "gc.created_by",
            "uu.name AS created_by_name",
            "gc.date_created",
            "gc.modified_by",
            "uu_3835.name AS modified_by_name",
            "gc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "gc.icon = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "gc.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3835",
                "name": "#__users_users",
                "on": "gc.modified_by = uu_3835.id"
            }
        ]
    }
