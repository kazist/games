    "query": {
        "from": {
            "table": "Games\\Games\\Players\\Code\\Tables\\Players",
            "alias": "ggp",
            "name": "#__games_games_players"
        },
        "select": [
            "ggp.id",
            "ggp.user_id",
            "uu.name AS user_id_name",
            "ggp.game_id",
            "gg.title AS game_id_title",
            "ggp.hit",
            "ggp.date_created",
            "ggp.created_by",
            "uu_3549.name AS created_by_name",
            "ggp.date_modified",
            "ggp.modified_by",
            "uu_7023.name AS modified_by_name"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ggp.user_id = uu.id"
            },
            {
                "table": "Games\\Games\\Code\\Tables\\Games",
                "alias": "gg",
                "name": "#__games_games",
                "on": "ggp.game_id = gg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3549",
                "name": "#__users_users",
                "on": "ggp.created_by = uu_3549.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_7023",
                "name": "#__users_users",
                "on": "ggp.modified_by = uu_7023.id"
            }
        ]
    }
